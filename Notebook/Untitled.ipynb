   "source": [
    "import tensorflow as tf\n",
    "\n",
    "# Cek apakah TensorFlow mendeteksi GPU\n",
    "print(\"Versi TensorFlow:\", tf.__version__)\n",
    "print(\"GPU yang terdeteksi:\")\n",
    "gpus = tf.config.list_physical_devices('GPU')\n",
    "if gpus:\n",
    "    for gpu in gpus:\n",
    "        print(gpu)\n",
    "else:\n",
    "    print(\"Tidak ada GPU yang terdeteksi.\")"
   ]
